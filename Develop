class Alumno:
    def __init__(self,nombre, nota):
        self.nombre= nombre
        self.nota=nota
    def str(self):
        return f"{self.nombre} {self.nota}\n"

    def hola(self):
        if int(self.nota)>=10:

            print(f"Hola, tu nombre es {self.nombre}, y sacaste {self.nota}, por eso aprobaste la materia")
        else:
            print(f"Hola, tu nombre es {self.nombre}, y sacaste {self.nota} Reprobaste tonto")
            
    

    

Santiago=Alumno("Santi", 10)
Santiago.hola()
